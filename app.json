{
	"expo": {
		"name": "wordsearch-photo-solver",
		"slug": "wordsearch-photo-solver",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"androidNavigationBar": {
			"backgroundColor": "#262B35"
		},
		"android": {
			"package": "com.labdevelopments.wordsearchphotosolver"
		}
	}
}